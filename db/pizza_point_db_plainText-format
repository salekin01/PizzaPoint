	SET ingredient_id = p_ingredient_id, 
		quantity_per_unit = p_quantity_per_unit, 
		unit_price = p_unit_price,
		units_in_stock = p_units_in_stock,
		hidden = p_hidden,
		updated_date = CURRENT_TIMESTAMP
	WHERE baker_goods_id = p_baker_goods_id;
    COMMIT;
	p_success = 1;
END;
$$;


