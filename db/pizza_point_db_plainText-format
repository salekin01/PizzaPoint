CREATE FUNCTION public.fn_baker_get_all() RETURNS SETOF public.baker
    LANGUAGE plpgsql
    AS $$
BEGIN
	RETURN QUERY
	SELECT *	
	FROM baker b
	ORDER BY b.baker_id DESC;
END;
$$;


